<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/lumine_ripple">
    <item android:id="@android:id/mask">
        <shape>
            <solid android:color="#FFFFFFFF" />
            <corners
                android:topRightRadius="@dimen/lumine_card_radius"
                android:topLeftRadius="@dimen/lumine_card_radius"
                android:bottomLeftRadius="@dimen/lumine_card_radius"
                android:bottomRightRadius="@dimen/lumine_card_radius" />
        </shape>
    </item>
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <solid android:color="@color/lumine_card_bg" />
            <corners
                android:topRightRadius="@dimen/lumine_card_radius"
                android:topLeftRadius="@dimen/lumine_card_radius"
                android:bottomLeftRadius="@dimen/lumine_card_radius"
                android:bottomRightRadius="@dimen/lumine_card_radius" />
        </shape>
    </item>
</ripple>
